#include <iostream>
#include <ctime>
#include <cstdlib>

using namespace std;

void insertSort(int arr[], int n) {
    int key, j, i;
    for (j = 1; j < n; j++) {
        key = arr[j];
        i = j - 1;
        while (i >= 0 && arr[i] > key) {
            arr[i + 1] = arr[i];
            i = i - 1;
        }
        arr[i + 1] = key;
    }
}

int main() {
    int n = 10000;
    int arr[n];
    srand(time(NULL));
    for (int i = 0; i < n; i++) {
        arr[i] = rand() % 0x7ffffffe;
    }

    clock_t start = clock();
    insertSort(arr, n);
    clock_t end = clock();

    cout << "Time taken for InsertSort: " << (double)(end - start) / CLOCKS_PER_SEC << " seconds" << endl;

    return 0;
}
